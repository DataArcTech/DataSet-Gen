# OpenAI-compatible LLM endpoint (used by generation + LLM-judge evaluation)
OPENAI_API_KEY=
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_CHAT_MODEL=gpt-4o-mini

# MinerU parsing service
MINERU_SERVER_URL=http://127.0.0.1:18899
MINERU_TIMEOUT_S=7200

# Optional: HTTP retry knobs (long runs may hit transient disconnects)
OPENAI_HTTP_MAX_RETRIES=6
OPENAI_HTTP_RETRY_BASE_S=1.0
MINERU_CLIENT_PROGRESS=1
MINERU_CLIENT_RETRY_BASE_S=1.0

